	<Image x:Name="wellnetLogo" Source="WellNetLogo.png"></Image>
    
    <Image x:Name="patient" Source="resident.png">
   <Image.GestureRecognizers>
        <TapGestureRecognizer Command="{Binding patientButtonCommand}"/>
    </Image.GestureRecognizers>
    </Image>
